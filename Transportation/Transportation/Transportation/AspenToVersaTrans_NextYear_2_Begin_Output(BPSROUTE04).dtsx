                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  atd.StudentNo ,
        --atd.program ,
        atd.LastName ,
        atd.FirstName ,
        atd.MiddleName ,
        atd.Grade ,
        atd.sch ,
        atd.streetno ,
        atd.street ,
        atd.apt ,
        atd.city ,
        atd.state ,
        atd.zip ,
        atd.DOB ,
        atd.parent1phones ,
        atd.parent2phones ,
        atd.emergencycontactphones ,
        atd.parent1name ,
        atd.parent2name ,
        atd.emergencycontactname ,
        atd.sex ,
        '70' [program] ,
        atd.PickupHouseNumber ,
        atd.PickupStreetname ,
        atd.PickupApartmentNumber ,
        atd.PickupCity ,
        atd.PickupState ,
        atd.Pickupzip ,
        atd.DropOffHouseNumber ,
        atd.DropOffStreetname ,
        atd.DropOffApartmentNumber ,
        atd.DropOffCity ,
        atd.DropOffState ,
        atd.DropOffzip ,
        atd.MedicaData ,
        atd.sncode ,
        atd.bilingual ,
        atd.aw ,
        atd.voced ,
        atd.theme ,
        atd.monitor ,
        atd.hzone ,
        atd.zone ,
        atd.geo ,
        atd.hphone ,
        atd.BufferZone ,
        atd.HomeRoom ,
        '1' [isWithDrawn],
        --atd.IsWithDrawn ,
        atd.ishomeless ,
        atd.IsProtectedAddress ,
        atd.TapCardID,
        atd.atdFamilyId [ctdFamilyId]
FROM    Transportation.ArchiveTransportationNext atd
        LEFT JOIN Transportation.CurrentTransportationNext ctd ON atd.StudentNo = ctd.StudentNo
-- these are yesterday's valid student transportation records
-- that are invalid today, or which are missing entirely
WHERE   ( ctd.StudentNo IS NULL
          OR ctd.program = '00'
          OR ctd.zip = ''
          OR ctd.street = ''
          OR ctd.city IS NULL
        )
        AND ( NOT atd.program = '00'
              AND NOT atd.zip = ''
              AND NOT atd.street = ''
              AND NOT atd.city IS NULL
            )
-- use UNION ALL to put these differences at the top of the result set
UNION ALL
( SELECT    StudentNo ,
            LastName ,
            FirstName ,
            MiddleName ,
            Grade ,
            sch ,
            streetno ,
            street ,
            apt ,
            city ,
            state ,
            zip ,
            DOB ,
            parent1phones ,
            parent2phones ,
            emergencycontactphones ,
            parent1name ,
            parent2name ,
            emergencycontactname ,
            sex ,
            program ,
            PickupHouseNumber ,
            PickupStreetname ,
            PickupApartmentNumber ,
            PickupCity ,
            PickupState ,
            Pickupzip ,
            DropOffHouseNumber ,
            DropOffStreetname ,
            DropOffApartmentNumber ,
            DropOffCity ,
            DropOffState ,
            DropOffzip ,
            MedicaData ,
            sncode ,
            bilingual ,
            aw ,
            voced ,
            theme ,
            monitor ,
            hzone ,
            zone ,
            geo ,
            hphone ,
            BufferZone ,
            HomeRoom ,
            IsWithDrawn ,
            ishomeless ,
            IsProtectedAddress ,
            TapCardID,
            ctdFamilyId
  FROM      Transportation.CurrentTransportationNext 
  WHERE     NOT program = '00'
            AND NOT zip = ''
            AND NOT street = ''
            AND NOT city IS NULL
  EXCEPT
  SELECT    StudentNo ,
            LastName ,
            FirstName ,
            MiddleName ,
            Grade ,
            sch ,
            streetno ,
            street ,
            apt ,
            city ,
            state ,
            zip ,
            DOB ,
            parent1phones ,
            parent2phones ,
            emergencycontactphones ,
            parent1name ,
            parent2name ,
            emergencycontactname ,
            sex ,
            program ,
            PickupHouseNumber ,
            PickupStreetname ,
            PickupApartmentNumber ,
            PickupCity ,
            PickupState ,
            Pickupzip ,
            DropOffHouseNumber ,
            DropOffStreetname ,
            DropOffApartmentNumber ,
            DropOffCity ,
            DropOffState ,
            DropOffzip ,
            MedicaData ,
            sncode ,
            bilingual ,
            aw ,
            voced ,
            theme ,
            monitor ,
            hzone ,
            zone ,
            geo ,
            hphone ,
            BufferZone ,
            HomeRoom ,
            IsWithDrawn ,
            ishomeless ,
            IsProtectedAddress ,
            TapCardID,
            atdFamilyId
  FROM      Transportation.ArchiveTransportationNext 
)
--ORDER BY StudentNo</property>
